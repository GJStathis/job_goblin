{
  "manifest_version": 3,
  "name": "Job Page Saver",
  "version": "1.0.0",
  "description": "Save job pages to your database",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}